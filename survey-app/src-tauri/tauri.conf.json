{
  "$schema": "https://raw.githubusercontent.com/nickel-org/nickel.rs/master/",
  "productName": "Survey Helper",
  "identifier": "co.defenestration.survey",
  "version": "0.1.0",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Survey Helper",
        "width": 1100,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/256x256.png",
      "icons/512x512.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
